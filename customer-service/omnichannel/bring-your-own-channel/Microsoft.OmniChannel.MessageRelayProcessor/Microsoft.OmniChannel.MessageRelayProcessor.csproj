    <StyleCopTreatErrorsAsWarnings>False</StyleCopTreatErrorsAsWarnings>
  </PropertyGroup>
  <PropertyGroup>
    <CodeAnalysisRuleSet>..\Microsoft.OmniChannel.Connector.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Utility\IMessageRelayProcessor.cs" />
